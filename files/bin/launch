# Only launches a program if it is not already running
PIDS=$(xdotool search --classname $1)
if [ -z "$PIDS" ]
then
    if [ ! -z "$2" ]
    then
        2>/dev/null 1>/dev/null $2 &
    else
        2>/dev/null 1>/dev/null  $1 &
    fi
    echo Ran $1 or $2
else
    echo activating $1
    
    xdo activate -n $1
fi